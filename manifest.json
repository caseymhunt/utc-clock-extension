{
	"manifest_version": 3,
	"name": "GMT/UTC Clock",
	"description": "UTC Clock extension for Google Chrome.",
	"version": "5.1.0",
	"author": "Casey Hunt",
	"background": {
		"service_worker": "sw.js"
	},
	"permissions": [
		"alarms"
	],
	"action": {
		"default_title": "UTC Clock",
		"default_icon": "images/icon2.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"128": "images/icon_128.png",
		"48": "images/icon_48.png"
	}
}